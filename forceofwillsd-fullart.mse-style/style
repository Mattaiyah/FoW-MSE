				else if card.cardtype == "regalia" then true
				else if card.cardtype != "resonator" then false
			}
		bottom:
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 59.63
				else if styling.card_format == "Lapis Cluster" then 55.61
				else if card.cardtype == "ruler" then 554.09
				else if card.cardtype == "jruler" then 506.52
				else if card.cardtype == "resonator" then 49.58
				else if card.cardtype == "spell" then 49.58
				else if card.cardtype == "regalia" then 66.33
				else if card.cardtype == "stone" then 549.4
			}
		left: 
			{
				if card.cardtype == "resonator" then 115.24
				else if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 44.89
				else if card.cardtype == "spell" then 115.24
				else if card.cardtype == "stone" then 33.5
				else if card.cardtype == "ruler" then 33.5
				else if card.cardtype == "jruler" then 33.5
				else if card.cardtype == "regalia" then 115.24
			}
		width: 
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 343.04
				else if card.cardtype == "ruler" then 296.14
				else if card.cardtype == "jruler" then 369.84
				else if card.cardtype == "resonator" then 288.1
				else if card.cardtype == "spell" then 288.1
				else if card.cardtype == "regalia" then 288.1
				else if card.cardtype == "stone" then 365.15
			}
		height: 26.8
		alignment: 
			{
				if styling.card_format == "Lapis Cluster" then "middle center stretch if-overflow"
				else if card.cardtype == "ruler" then "top center stretch if-overflow"
				else if card.cardtype == "jruler" then "top center stretch if-overflow"
				else if card.cardtype == "resonator" then "stretch if-overflow"
				else if card.cardtype == "spell" then "stretch if-overflow"
				else if card.cardtype == "regalia" then "stretch if-overflow"
				else if card.cardtype == "stone" then "top center stretch if-overflow"
			}
		font:
			name: Constantia
			size: 15.41
			color: black
			shadow color: 
				{
					if card.rarity == "SR" then "black"
					else if card.cardtype == "regalia" then "black"
